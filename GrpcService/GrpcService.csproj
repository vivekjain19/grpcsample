	<ItemGroup>
		<PackageReference Include="Grpc.AspNetCore" Version="2.40.0" />
		<PackageReference Include="Grpc.AspNetCore.Web" Version="2.47.0" />
	</ItemGroup>

	<ItemGroup>
		<None Update="Protos\users.proto" GrpcServices="None" />
	</ItemGroup>

	<ProjectExtensions>
		<VisualStudio>
